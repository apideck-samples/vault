    "next-page-tester": "^0.20.0",
    "postcss": "^8.4.31",
    "prettier": "^2.2.1",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --config .prettierrc 'src/**/*.ts' --write",
      "eslint ./ --ext ts,js --cache --fix"
    ]
  }
