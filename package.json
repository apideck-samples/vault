    "@headlessui/react": "^1.7.17",
    "axios": "^1.6.0",
    "camelcase-keys-deep": "^0.1.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "fuse.js": "^6.5.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mdi-react": "^7.4.0",
    "next": "^13.5.4",
    "node-fetch": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys": "^2.0.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^6.0.3",
    "react-select": "^4.3.1",
    "react-table": "^7.7.0",
    "react-transition-group": "^4.4.2",
    "react-waypoint": "^10.1.0",
    "rehype-raw": "^5.1.0",
    "swr": "^2.2.4"
